<%= render partial: "layouts/error", locals: {object: @home}  %>
<%= form_for @home do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %><br>
  <%= f.label :year_built %>
  <%= f.text_field :year_built, required: true %><br>
  <%= f.label :SF %>
  <%= f.text_field :sf, required: true %><br>
  <%= f.label :beds %>
  <%= f.text_field :beds, required: true %><br>
  <%= f.label :baths %>
  <%= f.text_field :baths, required: true %>
  <br>
  <br>
  <div>
    <%= f.submit "Submit Home", class: 'button'%>
  </div>

<% end %>
    <%= button_to "X Cancel", homes_path, :method => :get, class: 'button'%>
