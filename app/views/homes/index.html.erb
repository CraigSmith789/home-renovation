<h3>List Of Current Home Projects</h3>

<% @homes.each do |home| %>
      <b>Name: </b> <%= home.name %>
      <b>Year Built: </b> <%= home.year_built %>
      <b>SF: </b> <%= home.sf %>
      <b>Beds: </b> <%= home.beds %>
      <b>Baths: </b> <%= home.baths %>
      <%= link_to "View #{home.name}", home_path(home) %><br>

  <% end %>